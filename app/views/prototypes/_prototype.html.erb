<div class="card">
  <%= link_to image_tag(prototype.image, class: :card__img ), root_path%>
  <div class="card__body">
    <%= link_to prototype.title, root_path, class: :card__title%>
    <p class="card__summary">
    <li>
    <%= link_to '詳細', prototype_path(prototype.id), method: :get %>
    </li>
      <%= prototype.catch_copy %>
    </p>
    <%= link_to "by プロトタイプの投稿者名", user_path(prototype.user), class: :card__user %>
  </div>
</div>